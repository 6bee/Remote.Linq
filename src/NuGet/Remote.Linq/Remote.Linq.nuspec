    <tags>LINQ expression-tree lambda dynamic remote remote-query multi-tier n-tier fluent-interface</tags>
    <dependencies>
      <dependency id="aqua-core" version="[2.1.0,)" />
    </dependencies>
  </metadata>
  <files>  
    <file src="..\..\Remote.Linq\Remote.Linq\bin\Release\Remote.Linq.*" target="lib\net45" />
    <file src="..\..\Remote.Linq\Remote.Linq.NET35\bin\Release\Remote.Linq.*" target="lib\net35-client" />
    <file src="..\..\Remote.Linq\Remote.Linq.SL5\bin\Release\Remote.Linq.*" target="lib\sl5" />
    <file src="..\..\Remote.Linq\Remote.Linq.Win\bin\Release\Remote.Linq.*" target="lib\win" />
    <file src="..\..\Remote.Linq\Remote.Linq.WP\bin\Release\Remote.Linq.*" target="lib\wp8" />
    <file src="..\..\Remote.Linq\**\*.cs" target="src" exclude="..\..\**\bin\**;..\..\**\obj\**;..\..\**\*.Tests\**;..\..\Remote.Linq\Remote.Linq.QueryFunctions\**" />
  </files>
</package>