<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.serviceModel>
    <services>
      <service name="Server.DataService">
        <endpoint address="net.tcp://localhost:8080/dataservice" binding="netTcpBinding" contract="Common.ServiceContract.IDataService"/>
      </service>
      <service name="Server.RemoteLinqDataService">
        <endpoint address="net.tcp://localhost:8080/remotelinqdataservice" binding="netTcpBinding" contract="Common.ServiceContract.IRemoteLinqDataService"/>
      </service>
    </services>
  </system.serviceModel>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
</configuration>
